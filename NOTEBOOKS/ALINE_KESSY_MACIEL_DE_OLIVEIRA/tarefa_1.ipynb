    "#Lista de valores\n",
    "valores = [4, 6, 3, 4, 5, 8, 4, 2]\n",
    "\n",
    "# Calculando o valor máximo\n",
    "valor_maximo = max(valores)\n",
    "\n",
    "print(valor_maximo)\n",
    "\n",
    "# Calculando o valor mínimo\n",
    "valor_minimo = min(valores)\n",
    "\n",
    "print(valor_minimo)\n",
    "\n",
    "# Calculando a média aritmética\n",
    "soma = sum(valores)\n",
    "quantidade = len(valores)\n",
    "media = soma / quantidade\n",
    "\n",
    "print(media)\n",
    "\n",
    "# Calculando a mediana\n",
    "valores_ordenados = sorted(valores)\n",
    "\n",
    "#Comprimento da lista\n",
    "n = len(valores_ordenados)\n",
    "\n",
    "# Calcular a mediana\n",
    "if n % 2 == 1:\n",
    "    # Se o número de elementos for ímpar, a mediana é o valor central\n",
    "    mediana = valores_ordenados[n // 2]\n",
    "else:\n",
    "    # Se o número de elementos for par, a mediana é a média dos dois valores centrais\n",
    "    mediana = (valores_ordenados[n // 2 - 1] + valores_ordenados[n // 2]) / 2\n",
    "    \n",
    "print(mediana)\n",
    "\n",
    "# Calculando o desvio padrão\n",
    "media = sum(valores) / len(valores)\n",
    "\n",
    "# Calcular a soma dos quadrados das diferenças em relação à média\n",
    "soma_dos_quadrados = sum((x - media) ** 2 for x in valores)\n",
    "\n",
    "def raiz_quadrada(x, precisao=1e-10):\n",
    "    if x < 0:\n",
    "        raise ValueError(\"Não é possível calcular a raiz quadrada de um número negativo.\")\n",
    "    estimativa = x\n",
    "    while abs(estimativa * estimativa - x) > precisao:\n",
    "        estimativa = (estimativa + x / estimativa) / 2\n",
    "    return estimativa\n",
    "\n",
    "print(soma_dos_quadrados)"
